				// View Profile
			}else if(choice == 2){
				// View/Add Courses
			}else if(choice == 3){
				// Enroll/Drop A Student
			}else if(choice == 4){
				
			}else if(choice == 5){
				// Logout
			}else{
				consoleManager.displayInvalidChoiceError();
			}
		}
	}
	
