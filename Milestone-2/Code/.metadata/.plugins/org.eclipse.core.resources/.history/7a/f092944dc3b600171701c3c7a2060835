enum ExerciseMode{
	Adaptive, Random;
}
class Exercise {
	
	private ExerciseMode exerciseMode;
	
	private final String startDate, endDate, name;
	private int numQuestions, numRetries;
	
	private Exercise(ExerciseMode exerciseMode){
		this.exerciseMode = exerciseMode;
	}

	public ExerciseMode getExerciseType() {
		return exerciseMode;
	}
	
	
}
